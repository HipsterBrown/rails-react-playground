{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/playground-core.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": true,
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui --passWithNoTests",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prepare": "pnpm build",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@playground/core",
  "author": "Nick Hehr",
  "module": "dist/playground-core.mjs",
  "devDependencies": {
    "@preact/preset-vite": "^2.4.0",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-backgrounds": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-measure": "^6.5.12",
    "@storybook/addon-outline": "^6.5.12",
    "@storybook/addons": "^6.5.12",
    "@storybook/builder-vite": "^0.2.4",
    "@storybook/channel-postmessage": "^6.5.12",
    "@storybook/channel-websocket": "^6.5.12",
    "@storybook/client-api": "^6.5.12",
    "@storybook/preview-web": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitest/ui": "^0.24.0",
    "eslint": "^8.25.0",
    "eslint-config-preact": "^1.3.0",
    "husky": "^4.2.5",
    "jsdom": "^20.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.0",
    "vite": "^3.0.0",
    "vitest": "^0.24.0",
    "vite-plugin-preact-islands": "workspace:*"
  },
  "dependencies": {
    "@github/catalyst": "^1.6.0",
    "preact": ">= 10"
  }
}
